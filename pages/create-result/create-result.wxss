/* pages/create-result/create-result.wxss */
page {
    height: 100%;
}

.page-container {
    display: flex;
    flex-direction: column;
}

.section {
    margin-top: 25px;
    display: flex;
    justify-content: center;
}

.text-room-title {
    color: #cccccc;
    font-size: 11pt; 
}

.text-room-value {
    color: #09BB07;
    font-size: 35pt;
}

.text-success-title {
    margin-top: 5px;
    color: #353535;
}

.text-friendly-reminder {
    margin-left: 15px;
    text-align: center;
    margin-right: 15px;
    font-size: 10pt;
    color: #999999;
}

.btn-area {
    margin-top: 25px;
}

.section-footer {
    flex: auto;
    display: flex;
    justify-content: center;
    align-items: flex-end;
}

.text-god-view {
    text-align: center;
    font-size: 10pt;
    color: #576B95;
    margin-bottom: 25px;
}

